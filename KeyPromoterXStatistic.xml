<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find in Path...">
        <Item>
          <IdeaActionID>FindInPath</IdeaActionID>
          <ShortCut>'⇧⌘F'</ShortCut>
          <Description>Find in Path...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'⌘N' or '⌃⏎'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'⇧⌘F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Split Vertically">
        <Item>
          <IdeaActionID>SplitVertically</IdeaActionID>
          <ShortCut>'⌃L'</ShortCut>
          <Description>Split Vertically</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>