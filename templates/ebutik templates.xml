<templateSet group="ebutik templates">
  <template name="rowElem" value="&lt;div class=&quot;rowElem&quot;&gt;&#10;    &lt;label&gt;&lt;/label&gt;&#10;    &lt;div class=&quot;formRight&quot;&gt;&#10;            &#10;    &lt;/div&gt;&#10;    &lt;div class=&quot;fix&quot;&gt;&lt;/div&gt;        &#10;&lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="widget" value="&lt;div class=&quot;widget first&quot;&gt;&#10;    &lt;div class=&quot;head&quot;&gt;&lt;h5 class=&quot;iImage&quot;&gt;$title$&lt;/h5&gt;&lt;/div&gt;&#10;    &lt;div class=&quot;rowElem&quot;&gt;&#10;        &lt;label&gt;$name$&lt;/label&gt;&#10;        &lt;div class=&quot;formRight&quot;&gt;&#10;&#10;        &lt;/div&gt;&#10;        &lt;div class=&quot;fix&quot;&gt;&lt;/div&gt;&#10;    &lt;/div&gt;&#10;&lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="capitalize(String)" defaultValue="" alwaysStopAt="true" />
    <variable name="title" expression="capitalize(String)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="eurl" value="&lt;?=webLocation?&gt;" description="" toReformat="false" toShortenFQNames="true" />
  <template name="dump" value="var_dump($1$);die();" description="" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="cblock" value="// $1$ starts&#10;&#10;// $1$ ends" description="" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>